{
  "__comment": [
    "This file is missing the Wallet UI canister. That is because the wallet canister ",
    "includes its own UI (by implementing the `retrieve` method). The wallet build ",
    "script runs webpack which builds the wallet UI canister, and then includes its ",
    "output in the WASM (through Rust). This is why you won't see an asset canister ",
    "here."
  ],
  "canisters": {
    "alice": {
      "build": "wallet/build.sh alice",
      "candid": "wallet/src/lib.did",
      "type": "custom",
      "wasm": "wallet/target/wasm32-unknown-unknown/release/wallet-alice.wasm"
    },
    "bob": {
      "build": "wallet/build.sh bob",
      "candid": "wallet/src/lib.did",
      "type": "custom",
      "wasm": "wallet/target/wasm32-unknown-unknown/release/wallet-bob.wasm"
    },
    "wallet": {
      "build": "wallet/build.sh main",
      "candid": "wallet/src/lib.did",
      "type": "custom",
      "wasm": "wallet/target/wasm32-unknown-unknown/release/wallet-main.wasm"
    }
  },
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ]
    },
    "local": {
      "bind": "127.0.0.1:8001",
      "type": "ephemeral"
    }
  },
  "version": 1
}
